-- Main Script - Language Selector
local LanguageSelector = loadstring(game:HttpGet("https://raw.githubusercontent.com/uyh505-eng/published-content-jycoui/refs/heads/main/yeshhhh"))()

-- Script links for each language
local scripts = {
	["English"] = "https://v0-supabase-secure-storage.vercel.app/api/script/73b94ec2e1bd24682bf13477f66d2747",
    ["Spanish"] = "https://v0-supabase-secure-storage.vercel.app/api/script/60b400b4d066bf56f2b9b2351caab2f9",
    ["Vietnamese"] = "https://v0-supabase-secure-storage.vercel.app/api/script/9cf7bbf70fd32505055ca71796a942c5",
    ["Filipino"] = "https://v0-supabase-secure-storage.vercel.app/api/script/f9db6196a221f70c50485e81cca87f31",
    ["Thai"] = "https://v0-supabase-secure-storage.vercel.app/api/script/3658251b68d003b202f8d4c0c70f7a5f",
    ["Russian"] = "https://v0-supabase-secure-storage.vercel.app/api/script/b4e0b5e94c2dbf23b7e8f2c5cb581ac3",
    ["Portuguese"] = "https://v0-supabase-secure-storage.vercel.app/api/script/b5f1be1de9ad1b85772a08ced9b95605",
    ["German"] = "https://v0-supabase-secure-storage.vercel.app/api/script/11e985b81038b46d78640b25f7fbacd4",
    ["Korean"] = "https://v0-supabase-secure-storage.vercel.app/api/script/2dd08683c10f322e16fdbdc7f8349365"
}

-- Initialize the language selector
local selector = LanguageSelector.new()
local autoLoaded = selector:Init(scripts)

-- If no language was auto-loaded, show the GUI
if not autoLoaded then
    selector:CreateGUI()
end
  
 
